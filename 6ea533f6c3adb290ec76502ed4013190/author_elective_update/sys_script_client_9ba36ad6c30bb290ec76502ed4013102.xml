        <field>end_date_time</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Calculate Fee and Validate Date</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }

    // 1. ดึงค่าเวลาเริ่มและเวลาสิ้นสุดจากหน้าจอ
    var startStr = g_form.getValue('start_date_time');
    var endStr = newValue; // newValue คือค่าล่าสุดของ end_date_time

    if (!startStr || !endStr) return;

    // 2. แปลงเป็นตัวเลขเพื่อเปรียบเทียบ (Time validation)
    var startDate = new Date(g_form.getFormControl('start_date_time').value).getTime();
    var endDate = new Date(g_form.getFormControl('end_date_time').value).getTime();

    // --- เงื่อนไขด่านหน้า: เช็กวันสิ้นสุดต้องไม่น้อยกว่าวันเริ่ม (ตามรูป 15.11.08) ---
    if (startDate >= endDate) {
        g_form.showFieldMsg('end_date_time', 'วันที่สิ้นสุดต้องมากกว่าวันที่เริ่มจองนะครับรัน!', 'error');
        g_form.setValue('parking_fee', 0);
        g_form.setValue('duration_hours', 0);
        return;
    }

    // 3. คำนวณชั่วโมง (ตรรกะเดียวกับ Business Rule ของรัน)
    var diffMs = endDate - startDate;
    var diffHours = diffMs / (1000 * 60 * 60);
    var durationHours = Math.ceil(diffHours);
    g_form.setValue('duration_hours', durationHours); //

    // 4. คำนวณค่าจอด (Progressive Rate)
    var parkingFee = 0;
    var fullDays = Math.floor(durationHours / 24);
    var remainingHours = durationHours % 24;

    if (fullDays > 0) parkingFee += (fullDays * 200);

    if (remainingHours > 0) {
        if (remainingHours > 8) {
            parkingFee += 200;
        } else {
            var hoursLeft = remainingHours;
            // Tier 1: 10฿ | Tier 2: 15฿/ชม | Tier 3: 20฿/ชม
            if (hoursLeft > 0) { parkingFee += 10; hoursLeft--; }
            if (hoursLeft > 0) { var t2 = Math.min(hoursLeft, 3); parkingFee += (t2 * 15); hoursLeft -= t2; }
            if (hoursLeft > 0) { parkingFee += (hoursLeft * 20); }
        }
    }

    // 5. แสดงผลราคาสุทธิบนหน้าจอ
    g_form.setValue('parking_fee', parkingFee);
}]]></script>
