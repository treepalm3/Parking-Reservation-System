        <messages/>
        <name>Validate Date on Submit</name>
        <order/>
        <script><![CDATA[function onSubmit() {
    // 1. ดึงค่าเวลาจากฟอร์ม
    var start = g_form.getValue('start_date_time');
    var end = g_form.getValue('end_date_time');

    if (start && end) {
        var startDate = new Date(g_form.getFormControl('start_date_time').value).getTime();
        var endDate = new Date(g_form.getFormControl('end_date_time').value).getTime();

        // 2. ถ้าเวลาผิดพลาด ให้แจ้งเตือนและส่งค่า false เพื่อหยุดการบันทึก
        if (startDate >= endDate) {
            g_form.addErrorMessage('บันทึกไม่ได้! วันที่สิ้นสุดต้องมากกว่าวันที่เริ่มจองนะครับรัน');
            return false; // การ return false ใน onSubmit จะทำให้กดบันทึกไม่สำเร็จครับ
        }
    }
    return true; // ถ้าถูกต้องให้บันทึกได้ปกติ
}]]></script>
