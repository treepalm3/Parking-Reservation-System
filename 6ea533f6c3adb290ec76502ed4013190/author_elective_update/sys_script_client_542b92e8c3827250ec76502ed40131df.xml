        <name>Auto-populate User Name</name>
        <order/>
        <script><![CDATA[function onLoad() {
   // 1. เช็คก่อนว่าช่องชื่อว่างอยู่ไหม (ถ้ามีข้อมูลแล้วไม่ต้องทับ)
   // *สำคัญ: เปลี่ยน 'requester_name' เป็นชื่อ Field จริงของคุณ (ลองคลิกขวาที่ช่องชื่อในฟอร์มเพื่อดูชื่อจริง)*
   var nameField = 'Requester Name'; 
   
   if (g_form.getValue(nameField) == '') {
      // 2. ดึงชื่อคนล็อกอินปัจจุบัน (เช่น Abel Tuter) มาใส่
      g_form.setValue(nameField, g_user.getFullName());
   }
   
   // หมายเหตุ: ส่วนอีเมลต้องใช้โค้ดขั้นสูง (GlideAjax) เดี๋ยวเราเอาชื่อให้ผ่านก่อนครับ
}]]></script>
